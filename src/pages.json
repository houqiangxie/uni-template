{
  "globalStyle": {
    "navigationBarTitleText": "监管执法",
    "navigationBarBackgroundColor": "@navBgColor",
    "navigationBarTextStyle": "@navTxtStyle",
    "backgroundColor": "@bgColor",
    "backgroundTextStyle": "@bgTxtStyle",
    "backgroundColorTop": "@bgColorTop",
    "backgroundColorBottom": "@bgColorBottom",
    "app-plus": {
      "bounce": "none",
      "titleNView": {
        "backButton": {
          "background": "#fff"
        }
      }
    }
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^wd-(.*)": "wot-design-uni/components/wd-$1/wd-$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/index",
      "type": "home",
      "style": {
        "navigationBarTitleText": "首页",
        "app-plus": {
          "bounce": "none",
          "titleNView": {
            "backButton": {
              "background": "#fff"
            }
          }
        }
      }
    },
    {
      "path": "pages/login",
      "type": "page"
    },
    {
      "path": "pages/webview",
      "type": "page"
    }
  ],
  "subPackages": [
    {
      "root": "pages-sub/pages",
      "pages": [
        {
          "path": "index",
          "type": "page"
        }
      ]
    }
  ]
}
